<?xml version="1.0" encoding="utf-8" ?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="10dp"
        app:cardBackgroundColor="@color/cardBgColor"
        app:cardCornerRadius="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ftikcetImage"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:background="@color/hintcolor"
                android:scaleType="fitXY" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"

                android:paddingTop="10dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/uploadDate"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fUploadDate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="06/07/2021"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"

                        app:cardBackgroundColor="@color/bgColor">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/uploadTime"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fUploadTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="10:04 PM"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="10dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/referenceID"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fUploadRefId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="22222"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/slots"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fUploadSlots"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="100"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="10dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="150dp"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/matchCategory"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="90dp"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fUploadMatchCategory"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="Morning"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="10dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/matchDate"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fMatchDate"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="01/01/2022"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/matchTime"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fMatchTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="9:00 AM"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="10dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/roomId"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fRoomId"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="Not Available"

                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_weight="1"
                    android:gravity="start"
                    android:orientation="horizontal">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        app:cardBackgroundColor="@color/bgColor"
                        app:cardElevation="5dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="@string/roomPass"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        style="@style/CustomCardViewStyle2"
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white">

                        <TextView
                            android:id="@+id/fRoomPass"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:padding="2dp"
                            android:text="Not Available"
                            android:textAlignment="center"
                            android:textColor="#232323"
                            android:textSize="14sp" />

                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

            </LinearLayout>

            <TextView
                android:id="@+id/map"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="5dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/poppins_light"
                android:includeFontPadding="false"
                android:text="@string/mapName"
                android:textAllCaps="true"
                android:textColor="@color/bgColor"
                android:textSize="20sp" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingTop="5dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:orientation="vertical">

                    <com.google.android.material.card.MaterialCardView
                        style="@style/prizes"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"

                        app:cardBackgroundColor="@color/bgColor">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_margin="8dp"
                            android:fontFamily="@font/poppins_light"
                            android:includeFontPadding="false"
                            android:text="Prizes"
                            android:textAlignment="center"
                            android:textAllCaps="true"
                            android:textColor="@color/white"
                            android:textSize="20sp" />

                    </com.google.android.material.card.MaterialCardView>


                    <com.google.android.material.card.MaterialCardView
                        style="@style/prizes2"
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"

                        app:cardBackgroundColor="@color/white">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_weight="1"
                                    android:text="1st:"
                                    android:textColor="#232323"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/f1stPrize"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="20dp"
                                    android:layout_weight="1"
                                    android:gravity="end"
                                    android:text="200"
                                    android:textColor="@color/vibrantColor"
                                    android:textSize="17sp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_weight="1"
                                    android:text="2nd:"
                                    android:textColor="#232323"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/f2ndPrize"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="20dp"
                                    android:layout_weight="1"
                                    android:gravity="end"
                                    android:text="100"
                                    android:textColor="@color/vibrantColor"
                                    android:textSize="17sp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_weight="1"
                                    android:text="3rd:"
                                    android:textColor="#232323"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/f3rdPrize"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="20dp"
                                    android:layout_weight="1"
                                    android:gravity="end"
                                    android:text="50"
                                    android:textColor="@color/vibrantColor"
                                    android:textSize="17sp" />

                            </LinearLayout>

                        </LinearLayout>

                    </com.google.android.material.card.MaterialCardView>


                </LinearLayout>

                <TextView
                    android:id="@+id/fEntryPrice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/poppins_light"
                    android:includeFontPadding="false"
                    android:text="@string/entryPrice"
                    android:textAlignment="center"
                    android:textColor="@color/bgColor"
                    android:textSize="20sp" />

            </LinearLayout>

            <android.widget.Button
                android:id="@+id/btnDelete"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="10dp"
                android:background="@drawable/ripple_effect"
                android:includeFontPadding="false"
                android:text="@string/purchase"
                android:textColor="@color/white" />


        </LinearLayout>

    </com.google.android.material.card.MaterialCardView>

</LinearLayout>